{
  "name": "harmbandstra/edexmllib",
  "type": "library",
  "description": "PHP Library for validating and importing Edexml files.",
  "keywords": [
    "edexml", "php", "library"
  ],
  "license": "GPL-3.0",
  "require": {
    "php": "^7.2 || ^8.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "goetas-webservices/xsd2php-runtime": "^0.2.16"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "goetas-webservices/xsd2php": "^0.4.6"
  },
  "autoload": {
    "psr-4": {
      "Edexml\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
